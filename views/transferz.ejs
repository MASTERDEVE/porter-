<!--  Home  -->


<div class="container2 text-center">
    <div class="row align-items-start">
      <div class="col">
        <h1 class="heading">We simplify urban logistics with smarter transport solutions, optimized routes, and seamless moving services, paving the way for a sustainable future.</h1>
      </div>
      <div class="col">
        <img class="home-img" src="images/6620.jpg" alt="Image of a moving truck and a person">
      </div>

    </div>
  </div>


  <!-- Input Section -->


<div class="form-container">
  <form class="form">
      <div class="form-row">
          <div class="form-group">
              <label for="pickup">Pickup Address <span class="required">*</span></label>
              <input type="text" id="pickup" placeholder="Sending from" required>
          </div>
          <div class="form-group">
              <label for="drop">Drop Address <span class="required">*</span></label>
              <input type="text" id="drop" placeholder="Sending to" required>
          </div>
      </div>
      <div class="form-row">
          <div class="form-group">
              <label for="phone">Phone Number <span class="required">*</span></label>
              <input type="text" id="phone" placeholder="Enter contact details" required>
          </div>
          <div class="form-group">
              <label for="name">Name <span class="required">*</span></label>
              <input type="text" id="name" placeholder="Enter your name" required>
          </div>
      </div>
      <div class="form-row">
          <div class="form-group full-width">
              <label for="description">What describes you best? <span class="required">*</span></label>
              <select id="description" required>
                  <option value="" disabled selected>Click to choose</option>
                  <option value="business">Business</option>
                  <option value="individual">Individual</option>
              </select>
          </div>
      </div>
      <div class="form-row">
          <div class="form-group full-width">
              <button type="submit" class="btn1">Get an estimate</button>
          </div>
      </div>
  </form>
</div>
  

    
</body>
</html>