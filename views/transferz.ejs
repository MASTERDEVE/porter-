    <title>Transferz</title>
</head>
<body>

    
      <nav class="navbar navbar-expand-lg custom-navbar">
        <div class="container-fluid">
            <div class="navbar-brand">
                <img class="logo" src="images/profile.jpg" alt="Logo" />
            </div>
    
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
    
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active nav-menu1" aria-current="page" href="#">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active nav-menu1" aria-current="page" href="#about">Orders</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active nav-menu1" aria-current="page" href="#courses">Invoices</a>
                    </li>
                </ul>
                <div class="dropdown">
                    <img src="images/6620.jpg" alt="Image" class="dropdown-toggle" data-bs-toggle="dropdown">
                    <ul class="dropdown-menu dropdown-menu-end">
                        <li><a class="dropdown-item" href="#">Profile</a></li>
                        <li><a class="dropdown-item" href="#">Accounts</a></li>
                        <li><a class="dropdown-item" href="#">Support</a></li>
