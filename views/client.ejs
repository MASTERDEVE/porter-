          <input type="email" class="form-control m-2" id="exampleInputEmail1" name="email" aria-describedby="emailHelp"/>
        </div>
  
        <div class="mb-3">
          <label for="exampleFormControlTextarea1" class="form-label p-2 h5" >Mobile Number <span class="required">*</span></label>
          <input type="number" class="form-control m-2" name="number" id="name"/>

        </div>
        <div class="mb-3">
          <label for="exampleFormControlTextarea1" class="form-label p-2 h5" >House Address <span class="required">*</span></label>
          <input type="text" class="form-control m-2" name="text" id="name"/>

        </div>
     
      <form action="/transferz" method="post">
        <button type="submit" class="btn btn-secondary m-3 mb-5 home-btn">Submit</button>
